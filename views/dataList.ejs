<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Students GPA</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="https://fonts.googleapis.com/css?family=Bilbo+Swash+Caps" rel="stylesheet"> 
    <link href="https://fonts.googleapis.com/css?family=Kalam" rel="stylesheet"> 

    <link rel="stylesheet" type="text/css" href="css/form.css">
    <script type="text/javascript" src="js/form.js" defer></script>
</head>
<body>
    <nav class="menu">
      <ul>
        <li>
          <a href="/">Students Info Form</a><br>
        </li>
        <li>
          <a href="/dataList">Data List</a>
        </li>
      </ul>
      </nav>
    </nav>
  <section>
    <table>
        <thead>
            <th>Student Number</th>
            <th>Semester-1</th>
            <th>Semester-2</th>
            <th>Semester-3</th>
            <th>CGPA</th>

        </thead>
        <tbody>
            <% 
                for (var mark of marks ) {
            %>
            <tr>
                <td> <%= mark.number %> </td>
                <td> <%= mark.semester1 %> </td>
                <td> <%= mark.semester2 %> </td>
                <td> <%= mark.semester3 %></td>
                <% var cgpa = (mark.semester1 + mark.semester2 + mark.semester3)/3 %>
                <td> <%= cgpa %></td>
            </tr>
            <%
                }
            %>
        </tbody>
    </table>
  </section>

</body>
 </html>